USE DATABASE_DADOS;
-- Profundidade -> m
-- Medidas -> micrômetros (µm)

CREATE TABLE DADOS_DIAMETRO_PROFUNDIDADE(
    ID SERIAL PRIMARY KEY,
    PROFUNDIDADE NUMERIC(16,2) NOT NULL,
    DIAMETRO NUMERIC(16,2) NOT NULL
);



