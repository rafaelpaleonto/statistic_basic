USE DATABASE_DADOS;

CREATE TABLE DADOS_PROFUNDIDADE(
	ID SERIAL PRIMARY KEY,
	PROFUNDIDADE DECIMAL(10,2),
	DIAMETRO DECIMAL(10,2)
);


